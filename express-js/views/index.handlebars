<h1>{{title}}</h1>
<form action="/api/users" method="POST" class="mb-4">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" name="name" class="form-control">
    </div>
        <div class="form-group">
        <label for="age">Age</label>
        <input type="text" name="age" class="form-control">
    </div>
    <input type="submit" value="Add Member" class="btn btn-primary btn-block">
</form>
<ul class="list-group">
    {{#each users}}
        <li class="list-group-item">{{this.name}}</li>
    {{/each}}
</ul>

<a href="/api/users" class="btn btn-light mt-4">Visit API</a>